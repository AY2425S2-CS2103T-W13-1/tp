<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.stage.*?>

<fx:root resizable="false" title="Notes" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/17.0.2-ea"
         xmlns:fx="http://javafx.com/fxml/1">
    <scene>
        <Scene>
            <VBox fx:id="noteMessageContainer" alignment="TOP_LEFT" prefHeight="394.0" prefWidth="728.0" spacing="10.0">
                <children>
                    <TextArea fx:id="noteTextArea" wrapText="true" editable="true" prefHeight="309.0" prefWidth="708.0" />
                </children>
                <opaqueInsets>
                    <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
                </opaqueInsets>
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
            </VBox>
        </Scene>
    </scene>
</fx:root>
